<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="in.sp.beans.Address" id="address" autowire-candidate="false">

		<property name="houseno" value="101" />
		<property name="city" value="indore" />
		<property name="pincode" value="452016" />

	</bean>
	<bean class="in.sp.beans.Address" id="addId" >

		<property name="houseno" value="101" />
		<property name="city" value="delhi" />
		<property name="pincode" value="452016" />

	</bean>

	<bean class="in.sp.beans.Student" id="stdId" autowire="byType">

		<property name="name" value="shivam pal" />
		<property name="rollno" value="105" />

	</bean>

</beans>